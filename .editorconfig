# EditorConfig specifying coding styles like charset, indent style,
# indent size as well as a final newline at the end of each file

root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

[package.json]
indent_size = 2
